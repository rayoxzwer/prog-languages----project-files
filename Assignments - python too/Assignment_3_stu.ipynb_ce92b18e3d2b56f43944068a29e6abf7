      "source": [
        "# COMPLETE THE FUNCTION BELOW\n",
        "\n",
        "def naive_bayes_predict(train_data, train_labels, test_data, prior = 0.5):\n",
        "    train_class_1 =\n",
        "    train_class_2 =\n",
        "    mean_class_1, std_class_1 = np.mean(train_class_1), np.std(train_class_1)\n",
        "    mean_class_2, std_class_2 = np.mean(train_class_2), np.std(train_class_2)\n",
        "\n",
        "    prior_class_1 = len(train_class_1) / len(train_data)\n",
        "    prior_class_2 = len(train_class_2) / len(train_data)\n",
        "\n",
        "\n",
        "    # Predict class based on which posterior probability is larger\n",
        "    predictions = np.where(posterior_class_1 > posterior_class_2, 0, 1)\n",
        "    predictions = np.array(predictions).flatten()\n",
        "\n",
        "    return predictions"
      ],
